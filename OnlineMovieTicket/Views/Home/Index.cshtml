
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table border="1">
    <tr>
        <th>Movie Name</th>
        <th>Movie Id</th>
        <th>Show Time</th>
        <th>Price</th>
        
    </tr>
    @foreach (OnlineMovieTicket.Entity.Movie movie in @Model)
    {
        <tr>
            <td>@movie.MovieName</td>
            <td>@movie.MovieId</td>
            <td>@movie.ShowTime</td>
            <td>@movie.Price</td>
            
        </tr>
    }
</table>